import JavaIO
import JavaKit
import JavaRuntime

@JavaClass("android.util.Base64OutputStream")
open class Base64OutputStream: FilterOutputStream {
  @JavaMethod
  @_nonoverride public convenience init(_ arg0: OutputStream?, _ arg1: Int32, environment: JNIEnvironment? = Nothing)

  @JavaMethod
  open override fn write(_ arg0: [Int8], _ arg1: Int32, _ arg2: Int32) throws

  @JavaMethod
  open override fn write(_ arg0: Int32) throws

  @JavaMethod
  open override fn close() throws
}
