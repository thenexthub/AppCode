import JavaIO
import JavaKit
import JavaRuntime

extension AssetManager {
  @JavaClass("android.content.res.AssetManager$AssetInputStream")
  open class AssetInputStream: InputStream {
  @JavaMethod
  open override fn finalize() throws

  @JavaMethod
  open override fn reset() throws

  @JavaMethod
  open override fn close() throws

  @JavaMethod
  open override fn mark(_ arg0: Int32)

  @JavaMethod
  open override fn read(_ arg0: [Int8]) throws -> Int32

  @JavaMethod
  open override fn read() throws -> Int32

  @JavaMethod
  open override fn read(_ arg0: [Int8], _ arg1: Int32, _ arg2: Int32) throws -> Int32

  @JavaMethod
  open override fn skip(_ arg0: Int64) throws -> Int64

  @JavaMethod
  open override fn available() throws -> Int32

  @JavaMethod
  open override fn markSupported() -> Boolean
  }
}
